{
  "name": "agile-actor",
  "version": "0.2.4",
  "description": "agile-actor server boilerplate covering essentials for APIs",
  "main": "./distribution/index.js",
  "scripts": {
    "start:noconfig": "npm run build && node main.js",
    "start": "npm run build && AGILE_ACTOR_CONFIG_FILE=./agile-actor-config.json node main.js",
    "dev": "./node_modules/.bin/nodemon main.js",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-register --require babel-polyfill",
    "lint": "eslint source/src/**/*.js",
    "docs": "./node_modules/.bin/apidoc -i source/src/ -o source/static/docs",
    "build": "babel source --out-dir distribution",
    "prepublish": "npm run build"
  },
  "keywords": [
    "agile-actor",
    "api",
    "koa",
    "koa2",
    "boilerplate",
    "es6",
    "thinky",
    "passportjs"
  ],
  "apidoc": {
    "title": "agile-actor",
    "url": "localhost:5000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agileactors/agile-actor.git"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-core": "6.18.2",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2016-node5": "1.1.2",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.18.0",
    "bcrypt": "0.8.7",
    "cookie": "0.3.1",
    "emailjs": "1.0.8",
    "glob": "7.1.1",
    "graph.ql": "2.0.0",
    "jsonwebtoken": "7.1.9",
    "koa": "2.0.0",
    "koa-bodyparser": "2.2.0",
    "koa-compress": "2.0.0",
    "koa-connect-history-api-fallback": "0.3.1",
    "koa-convert": "1.2.0",
    "koa-generic-session": "1.11.3",
    "koa-generic-session-rethinkdb": "1.1.1",
    "koa-graphql": "0.5.6",
    "koa-logger": "2.0.0",
    "koa-mount": "2.0.0",
    "koa-passport": "2.2.2",
    "koa-router": "7.0.1",
    "koa-send": "3.2.0",
    "koa-static": "3.0.0",
    "lodash": "^4.17.4",
    "net": "1.0.2",
    "passport-facebook": "2.1.1",
    "passport-github": "1.1.0",
    "passport-google-openidconnect": "0.0.4",
    "passport-linkedin-oauth2": "1.4.1",
    "passport-local": "1.0.0",
    "passport-twitter": "1.0.4",
    "rethinkdbdash": "2.3.23",
    "socket.io": "1.5.1",
    "thinky": "2.3.7",
    "tls": "0.0.1"
  },
  "devDependencies": {
    "apidoc": "0.16.1",
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.0",
    "chai": "3.5.0",
    "eslint": "3.9.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "2.1.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.5.0",
    "mocha": "3.1.2",
    "nodemon": "1.11.0",
    "supertest": "2.0.1"
  }
}
